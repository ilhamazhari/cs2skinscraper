{
  "manifest_version": 3,
  "name": "CS2 Skin Scraper",
  "version": "1.0",
  "permissions": ["tabs", "scripting", "storage", "activeTab"],
  "host_permissions": [
    "https://raw.githubusercontent.com/*",
    "http://localhost:3000/*",
    "https://steamcommunity.com/*",
    "https://avan.market/*",
    "https://www.c5game.com/*",
    "https://white.market/*",
    "https://waxpeer.com/*",
    "https://skinbaron.de/*",
    "https://shadowpay.com/*",
    "https://gamerpay.gg/*",
    "https://cs.money/*",
    "https://cs.deals/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [{
    "matches": ["https://*.steamcommunity.com/*", "https://*.waxpeer.com/*", "https://*.cs.money/*", "https://*.cs.deals/*", "https://*.avan.market/*", "https://*.c5game.com/*", "https://*.white.market/*", "https://*.skinbaron.de/*", "https://*.shadowpay.com/*", "https://*.gamerpay.gg/*", "https://raw.githubusercontent.com/*"],
    "js": ["content.js"],
    "run_at": "document_end"
  }]
}